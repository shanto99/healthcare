{
    "compilerOptions": {
      "target": "es6",                               
      "esModuleInterop": true,    
      "module": "esnext",                     
      "forceConsistentCasingInFileNames": true,            
      "strict": true,                                     
      "skipLibCheck": true,
      "jsx": "react-jsx",
      "allowJs": true,
      "allowSyntheticDefaultImports": true,
      "noFallthroughCasesInSwitch": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": false,
      "noEmit": false,
      "noImplicitAny": false                           
    }
  }